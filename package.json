{
  "name": "employee-calculator-brazil",
  "version": "1.0.0",
  "author": "Matheus Ribeiro",
  "private": true,
  "keywords": [
    "Employee",
    "Calculator",
    "Brazil"
  ],
  "license": "UNLICENSED",
  "main": "src/index.js",
  "scripts": {
    "start": "node index.js",
    "test:uniq": "NODE_PATH=./ mocha --colors --reporter spec",
    "test": "yarn test:uniq \"src/**/*.test.js\"",
    "lint": "standard -v \"src/**/*.js\" \"index.js\" | snazzy",
    "coverage": "nyc yarn test --include \"src/**/*.js\" --all",
    "validate": "yarn lint && yarn coverage"
  },
  "engines": {
    "node": ">=12.16.2",
    "npm": "6.14.4"
  },
  "nyc": {
    "statements": 95,
    "branches": 95,
    "functions": 95,
    "lines": 95,
    "check-coverage": true,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "node_modules/**/*"
    ]
  },
  "standard": {
    "globals": [
      "describe",
      "context",
      "it",
      "after",
      "afterEach",
      "before",
      "beforeEach"
    ]
  },
  "dependencies": {
    "moment": "^2.29.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^8.2.0",
    "nyc": "^15.1.0",
    "snazzy": "^9.0.0",
    "standard": "^16.0.0"
  }
}
